    <body>
        <div id="header" class="mui-col-md-12 mui-col-xs-12">
            <span class="mui-col-md-2 mui-col-md-offset-5 mui-col-xs-2 mui-col-xs-offset-0 mui--text-center">
                <a href="/" id="linkage-link">linkage</a>
            </span>
            <span class="mui-col-md-1 mui-col-md-offset-3 mui-col-xs-1 mui-col-xs-offset-1 mui--text-right header-link">
